{
  "mcpServers": {
    "forge": {
      "args": [
        "-y",
        "automagik-forge",
        "--mcp"
      ],
      "command": "npx"
    },
    "ask-repo-agent": {
      "type": "sse",
      "url": "https://mcp.deepwiki.com/sse"
    },
    "search-repo-docs": {
      "command": "npx",
      "args": [
        "-y",
        "@upstash/context7-mcp"
      ]
    },
    "playwright": {
      "command": "npx",
      "args": ["-y", "@executeautomation/playwright-mcp-server"]
    }
  }
}
