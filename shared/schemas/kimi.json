{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "properties": {
    "append_prompt": {
      "title": "Append Prompt",
      "description": "Extra text appended to the prompt",
      "type": [
        "string",
        "null"
      ],
      "format": "textarea",
      "default": null
    },
    "model": {
      "description": "Model to use (e.g., \"kimi-k2\", \"kimi-k2.5\")",
      "type": [
        "string",
        "null"
      ]
    },
    "agent": {
      "description": "Agent type (e.g., \"default\", \"okabe\", or custom agent file)",
      "type": [
        "string",
        "null"
      ]
    },
    "skills": {
      "description": "Skills to load",
      "type": [
        "array",
        "null"
      ],
      "items": {
        "type": "string"
      }
    },
    "agent_file": {
      "description": "Custom agent file path",
      "type": [
        "string",
        "null"
      ]
    },
    "yolo": {
      "description": "YOLO mode - auto-approve all actions",
      "type": [
        "boolean",
        "null"
      ]
    },
    "base_command_override": {
      "title": "Base Command Override",
      "description": "Override the base command with a custom command",
      "type": [
        "string",
        "null"
      ]
    },
    "additional_params": {
      "title": "Additional Parameters",
      "description": "Additional parameters to append to the base command",
      "type": [
        "array",
        "null"
      ],
      "items": {
        "type": "string"
      }
    },
    "env": {
      "title": "Environment Variables",
      "description": "Environment variables to set when running the executor",
      "type": [
        "object",
        "null"
      ],
      "additionalProperties": {
        "type": "string"
      }
    }
  },
  "description": "Kimi CLI executor configuration",
  "type": "object"
}