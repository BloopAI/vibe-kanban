{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "title": "Gemini",
  "description": "An executor that uses Gemini to process tasks",
  "type": "object",
  "required": [
    "model"
  ],
  "properties": {
    "additional_params": {
      "type": [
        "array",
        "null"
      ],
      "items": {
        "type": "string"
      }
    },
    "append_prompt": {
      "type": [
        "string",
        "null"
      ]
    },
    "base_command_override": {
      "type": [
        "string",
        "null"
      ]
    },
    "model": {
      "$ref": "#/definitions/GeminiModel"
    },
    "yolo": {
      "type": [
        "boolean",
        "null"
      ]
    }
  },
  "definitions": {
    "GeminiModel": {
      "description": "Model variant of Gemini to use",
      "type": "string",
      "enum": [
        "default",
        "flash"
      ]
    }
  }
}